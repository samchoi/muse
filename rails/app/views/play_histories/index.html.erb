<h1>Listing play_histories</h1>

<table>
  <thead>
    <tr>
      <th>Audio</th>
      <th>User</th>
      <th>Data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @play_histories.each do |play_history| %>
      <tr>
        <td><%= play_history.audio_id %></td>
        <td><%= play_history.user_id %></td>
        <td><%= play_history.data %></td>
        <td><%= link_to 'Show', play_history %></td>
        <td><%= link_to 'Edit', edit_play_history_path(play_history) %></td>
        <td><%= link_to 'Destroy', play_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Play history', new_play_history_path %>
